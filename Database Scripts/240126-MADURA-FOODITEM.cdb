CREATE TABLE madura_fooditem_tab(
     item_id VARCHAR2(50) NOT NULL,
     available_qty NUMBER NOT NULL,
     scrapped_qty NUMBER NOT NULL,
     receved_qty NUMBER NOT NULL,
     item_name VARCHAR2(300) NOT NULL,
     PRIMARY KEY(item_id)
);

CREATE SEQUENCE fooditem_sequence
	START WITH 1
    INCREMENT BY 1
    NOCACHE
    NOCYCLE;