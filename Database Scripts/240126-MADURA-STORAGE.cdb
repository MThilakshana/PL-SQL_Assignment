CREATE TABLE madura_storage_tab(
     storage_id VARCHAR2(50) NOT NULL,
     item_id VARCHAR2(50) NOT NULL,
     container_id VARCHAR2(50) NOT NULL,
     status VARCHAR2(50) NOT NULL,
     PRIMARY KEY(storage_id,item_id,container_id)
);

CREATE SEQUENCE storage_sequence
	START WITH 1
    INCREMENT BY 1
    NOCACHE
    NOCYCLE;