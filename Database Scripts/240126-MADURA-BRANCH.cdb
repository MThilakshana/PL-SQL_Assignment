CREATE TABLE madura_branch_tab(
     branch_id VARCHAR2(50) NOT NULL,
     branch_name VARCHAR2(100) NOT NULL,
     branch_address VARCHAR2(250) NOT NULL,
     branch_manager VARCHAR2(100) NOT NULL,
     total_warehouse_capacity NUMBER NOT NULL,
     company_reg_no VARCHAR2(50) NOT NULL,
     PRIMARY KEY(branch_id,company_reg_no)
);

CREATE SEQUENCE branch_sequence
	START WITH 1
    INCREMENT BY 1
    NOCACHE
    NOCYCLE;